NitrogenClass.prototype.$show_disconnected_notice = function() {
    if(!this.$going_away && !this.$maybe_going_away) {
        var msg = "&#9889; Connection Broken! Attempting to reconnect... &#9889;";
        this.$show_notice_bar("error", msg);
